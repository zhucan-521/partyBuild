        select
        <include refid="LeadTeamVO_Column_List"/>
        ,
        <include refid="Blob_Column_List"/>
        , 2 as attachment_type,
        d.NAME AS org_name
        from tab_pb_lead_team l
        left join sys_dept d on l.org_id = d.dept_id
        where l.del_flag = 0 and lead_team_id = #{leadTeamId}
    </select>

