        </if>
        <if test="delFlag != null and delFlag != ''">
            AND p.del_flag = #{delFlag}
        </if>
        <if test="orderNum != null and orderNum != ''">
            AND p.order_num = #{orderNum}
        </if>
        <if test="description != null and description != ''">
            AND p.description = #{description}
        </if>
        <if test="createTime != null and createTime != ''">
            AND p.create_time = #{createTime}
        </if>
        <if test="createUserid != null and createUserid != ''">
            AND p.create_userid = #{createUserid}
        </if>
        <if test="createUsername != null and createUsername != ''">
            AND p.create_username = #{createUsername}
        </if>
        <if test="updateTime != null and updateTime != ''">
            AND p.update_time = #{updateTime}
        </if>
        <if test="updateUserid != null and updateUserid != ''">
            AND p.update_userid = #{updateUserid}
        </if>
        <if test="updateUsername != null and updateUsername != ''">
            AND p.update_username = #{updateUsername}
        </if>
