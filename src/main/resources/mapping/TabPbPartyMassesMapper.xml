  <select id="checkIsExistPartyMassesName" resultType="java.lang.Boolean">
    select
    count(1)
    from
    tab_pb_party_masses
    where
    del_flag = 0
    AND party_masses_name = #{partyMassesName}
    <if test="partyMassesId != null and partyMassesId != 0">
