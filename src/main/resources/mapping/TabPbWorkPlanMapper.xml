        </if>
        <if test="summaryCheckResult != null and summaryCheckResult != ''">
            AND p.summary_check_result = #{summaryCheckResult}
        </if>
        <if test="summaryCheckUser != null and summaryCheckUser != ''">
            AND p.summary_check_user = #{summaryCheckUser}
        </if>
        <if test="eblFlag != null and eblFlag != ''">
