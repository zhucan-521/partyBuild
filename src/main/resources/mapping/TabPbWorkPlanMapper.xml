    <sql id="Base_WhereCase_List">
        <if test="planId != null and planId != ''">
            AND p.plan_id = #{planId}
        </if>
        <if test="orgId != null and orgId != ''">
            AND p.org_id = #{orgId}
        </if>
        <if test="planYear != null and planYear != ''">
            AND p.plan_year = #{planYear}
        </if>
        <!--上报日期-->
        <if test="reportStartDate != null and reportStartDate != ''">
            AND date_format(p.report_date, '%Y-%m-%d') >= #{reportStartDate}
        </if>
        <if test="reportEndDate != null and reportEndDate != ''">
            AND date_format(p.report_date, '%Y-%m-%d') &lt;= #{reportEndDate}
        </if>
        <!--总结日期-->
        <if test="summaryStartDate != null and summaryStartDate != ''">
            AND date_format(p.summary_date, '%Y-%m-%d') >= #{summaryStartDate}
        </if>
        <if test="summaryEndDate != null and summaryEndDate != ''">
            AND date_format(p.summary_date, '%Y-%m-%d') &lt;= #{summaryEndDate}
        </if>
        <!--计划审核日期-->
        <if test="planCheckStartDate != null and planCheckStartDate != ''">
            AND date_format(p.plan_check_date, '%Y-%m-%d') >= #{planCheckStartDate}
        </if>
        <if test="planCheckEndDate != null and planCheckEndDate != ''">
            AND date_format(p.plan_check_date, '%Y-%m-%d') &lt; #{planCheckEndDate}
        </if>
        <!--总结审核日期-->
        <if test="summaryStartDate != null and summaryStartDate != ''">
            AND date_format(p.summary_check_date, '%Y-%m-%d') >= #{summaryStartDate}
        </if>
        <if test="summaryEndDate != null and summaryEndDate != ''">
