        <include refid="Blob_Column_List"/>
        , t.name as org_name,
        t2.name as check_org_name,
        u.realname as check_user_name,
        t3.name as summary_check_org_name,
        u2.realname as summary_check_user_name,
        '1' as attachment_type_plan,
        '4' as attachment_type_summary
        from tab_pb_work_plan p
        left join sys_dept t on p.org_id = t.dept_id
        left join sys_dept t2 on p.check_org = t2.dept_id
        left join sys_user u on p.check_user = u.user_id
        left join sys_dept t3 on p.summary_check_org = t3.dept_id
        left join sys_user u2 on p.summary_check_user = u2.user_id
        where p.plan_id = #{planId,jdbcType=BIGINT}
    </select>
